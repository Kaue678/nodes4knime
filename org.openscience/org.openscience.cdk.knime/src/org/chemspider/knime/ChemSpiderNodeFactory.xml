<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="./favicon.ico" type="Other">
	<name>ChemSpider</name>
	
	<shortDescription>
	    Download molecular structures from ChemSpider.com.
	</shortDescription>
	
	<fullDescription>
		<intro>Downloads MDL molfiles for molecular structures based
                  on given InChIKey's.</intro>
	</fullDescription>
	
	<ports>
		<inPort index="0" name="Table with InchiKeys">One or more InChIKeys.</inPort>
		<!-- possibly more ports here-->
		<outPort index="0" name="Table with Mol structures">MDL molfiles for the given InChIKeys</outPort>
	</ports>
</knimeNode>
